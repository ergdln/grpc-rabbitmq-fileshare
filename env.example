# RabbitMQ Configuration
RABBITMQ_USER=guest
RABBITMQ_PASS=guest
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# AMQP URL (usado pelos clientes e servidor RabbitMQ)
# Formato: amqp://user:pass@host:port/
AMQP_URL=amqp://guest:guest@rabbitmq:5672/

# gRPC Server Configuration
GRPC_SERVER_PORT=50051
GRPC_SERVER_ADDR=grpc-server:50051

# Storage Configuration
DATA_DIR=/data

# Client Scaling
GRPC_CLIENT_SCALE=1
RABBITMQ_CLIENT_SCALE=1

